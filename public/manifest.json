{
  "manifest_version": 2,
  "version": "2",

  "short_name": "React App",
  "name": "Create React App Sample",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
